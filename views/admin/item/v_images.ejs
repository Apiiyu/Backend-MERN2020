<div class="tab-pane fade show active" id="showImages" role="tabpanel" aria-labelledby="home-tab">
  <div class="row">
    <% data.images.map((item, index) => { %>
      <div class="col-md-4">
        <div class="card shadow mb-4">
          <div class="card-body">
            <img src="https://staycation-id.herokuapp.com/public/assets/uploads/items/<%= item.imageUrl %>" class="w-100 h-100 d-block" style="min-height: 240px;" alt="images">  
          </div>
        </div>
      </div>
    <% }) %>  
  </div>
</div>